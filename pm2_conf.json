{
    "name": "bsv20 indexer server",
    "script": "index.js",
    "watch": true,
    "ignore_watch": ["node_modules", "logs","public"],
    "watch_options": {
      "followSymlinks": false
    },
    "error_file": "logs/error.log",
    "out_file": "logs/output.log",
    "log_date_format": "YYYY-MM-DD HH:mm:ss.SSS",
    "time": true
  }